# mount    fstype    device
/system    ext4      /dev/block/mmcblk0p9
/cache     ext4      /dev/block/mmcblk0p7
/data      ext4      /dev/block/mmcblk0p10
/efs       ext4      /dev/block/mmcblk0p1     flags=display="EFS";backup=1
/boot      emmc      /dev/block/mmcblk0p5
/recovery  emmc      /dev/block/mmcblk0p6
/emmc      auto      /dev/block/mmcblk0p11    flags=display="Internal Storage";storage;wipeingui
/sdcard    auto      /dev/block/mmcblk1p1     /dev/block/mmcblk1    flags=display="MicroSD card";storage;wipeingui;removeable